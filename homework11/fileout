CC=gcc
CFLAGS=-ggdb -Wall

.PHONY: clean


wyls: wyshell.c ShellFunctions.c ShellFunctions.h wyscanner.c wyscanner.h
	${CC} ${CFLAGS} wyshell.c wyscanner.c ShellFunctions.c -o wyshell

clean: 
	/bin/rm -f *.o wyshell
